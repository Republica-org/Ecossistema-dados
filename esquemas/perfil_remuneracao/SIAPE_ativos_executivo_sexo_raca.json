{
  "view": {
    "type": "data_series",
    "range": [
      {
        "label": "Total de vínculos",
        "value": "quantidade_vinculos_sum"
      }
    ],
    "domain": "ano",
    "groupBy": [
      "genero",
      "cor_raca"
    ],
    "filterBy": [
      "genero",
      "cor_raca"
    ],
    "chartType": "lines",
    "dataSourceType": "aggregation"
  },
  "variables": {
    "ano": {
      "type": "continuous",
      "label": "Ano"
    },
    "genero": {
      "type": "categorical",
      "label": "Gênero",
      "options": [
        "Feminino",
        "Masculino"
      ],
      "phrasePrefix": "do gênero"
    },
    "cor_raca": {
      "type": "categorical",
      "label": "Cor/Raça",
      "options": [
        "Preta",
        "Branca",
        "Parda",
        "Amarela",
        "Índigena",
        "Não informado"
      ],
      "phrasePrefix": "da cor"
    },
    "quantidade_vinculos": {
      "type": "continuous",
      "label": "Total de vínculos"
    }
  }
}