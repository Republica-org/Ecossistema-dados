{
  "view": {
    "type": "data_series",
    "range": [
      {
        "label": "Total de vínculos",
        "value": "quantidade_vinculos_sum"
      }
    ],
    "domain": "ano_ingresso",
    "groupBy": [
      "cor_raca",
      "genero"
    ],
    "filterBy": [
      "genero",
      "cor_raca"
    ],
    "chartType": "lines",
    "defaultValues": {
      "filterBy": {
        "genero": [
          "Feminino",
          "Masculino"
        ]
      }
    },
    "dataSourceType": "aggregation"
  },
  "variables": {
    "genero": {
      "type": "categorical",
      "label": "Gênero",
      "options": [
        "Feminino",
        "Masculino"
      ],
      "phrasePrefix": "do gênero"
    },
    "cor_raca": {
      "type": "categorical",
      "label": "Raça",
      "options": [
        "Branca",
        "Não informada",
        "Negra",
        "Outros"
      ],
      "phrasePrefix": "da cor"
    },
    "ano_ingresso": {
      "type": "continuous",
      "label": "Ano de ingresso"
    },
    "quantidade_vinculos": {
      "type": "continuous",
      "label": "Total de vínculos ativos"
    }
  }
}