{
  "view": {
    "type": "br_map_continuous",
    "range": [
      {
        "label": "Quantidade de vinculos",
        "value": "quantidade_vinculos"
      },
      {
        "label": "Proporção de vínculos por tipo",
        "value": "proporcao_tipo"
      }
    ],

    "filterBy": [
      {
        "type": "single",
        "variableId": "tipo_vinculo"
      }
    ],
    "defaultValues": {
      "filterBy": {
        "tipo_vinculo": "Estatutários"
      }
    },
    "mapType": "states",
    "stateIdVariableId": "sigla_uf"
  },
  "variables": {
    "sigla_uf": {
      "type": "categorical",
      "categories": [],
      "variableId": "sigla_uf"
    },
    "total_estado": {
      "type": "continuous"
    },
    "ano": {
      "type": "continuous",
      "categories": [2021]
    },
    "quantidade_vinculos": {
      "type": "continuous"
    },
    "tipo_vinculo": {
      "type": "categorical",
      "label":"Tipode vínculo",
      "options": ["Estatutários","Celetistas", "Somente comissionados","Estagiários","Sem vínculo permanente"],
      "variableId": "tipo_vinculo",
      "phrasePrefix": " "
    }
  },
  


  "valueExpressions": {
    "quantidade_vinculos": "sum(quantidade_vinculos)",
    "proporcao_tipo": {
      "label": "Proporção por tipo de vínculo",
      "value": "sum(quantidade_vinculos) / sum(total_estado) "
    }
  }
}