{
  "view": {
    "type": "data_series",
    "range": [
      {
        "label": "Total de vínculos",
        "value": "quantidade_vinculos_sum"
      }
    ],
    "domain": "ano",
    "groupBy": [
      "natureza_juridica",
      "cor_raca"
    ],
    "filterBy": [
      "natureza_juridica",
      "cor_raca"
    ],
    "chartType": "lines",
    "dataSourceType": "aggregation"
  },
  "variables": {
    "ano": {
      "type": "continuous",
      "label": "Ano"
    },
    "cor_raca": {
      "type": "categorical",
      "label": "Raça",
      "options": [
        "Branca",
        "Não informada",
        "Negra",
        "Outros"
      ],
      "phrasePrefix": "da cor"
    },
    "natureza_juridica": {
      "type": "categorical",
      "label": "Natureza Jurídica",
      "options": [
        "Autarquia Federal",
        "Fundação Federal",
        "Poder Executivo Federal"
      ],
      "phrasePrefix": "da natureza jurídica denominada"
    },
    "quantidade_vinculos": {
      "type": "continuous",
      "label": "Total de vínculos ativos"
    }
  }
}