{
  "view": {
    "type": "data_series",
    "dataSourceType": "staticValues",
    "chartType": "bars",
    "dimensionVariableId": "variavel",
    "dimensionValueVariableId": "categoria",

    "domain": "flag_publico_privado",
    "range": ["media_remuneracao_ajustada", "media_remuneracao"],


    "filterBy": [
      {
        "type": "single",
        "variableId": "ano"
      }
    ],
    "defaultValues": {
      "filterBy": {
        "ano": 2021
      }
    },
    "numberFormat": {
      "style": "currency",
      "currency": "BRL"
    },

    "groupBy": [
      {
        "value": "genero",
        "label": "Gênero"
      },
      {
        "value": "poderes",
        "label": "Poderes"
      },
      {
        "value": "tipo_vinculo",
        "label": "Tipo de vínculo"
      },
      {
        "value": "nivel_governo",
        "label": "Nível de governo"
      },
      {
        "value": "grau_instrucao",
        "label": "Grau de instrução"
      }
    ]
  },
  "filterBy": [
    "flag_publico_privado"
  ],

  
  "variables": {
    "flag_publico_privado": {
      "type": "categorical",
      "label":"tipo de setor"
    },
    
    "ano": {
      "type": "continuous",
      "label": "Ano",
      "options": [
        2013,
        2014,
        2015,
        2016,
        2017,
        2018,
        2019,
        2020,
        2021
      ],
      "phrasePrefix": "em"
    },
    "variavel": {
      "type": "categorical"
    },
    "categoria": {
      "type": "categorical"
    },
    "media_remuneracao": {
      "type": "continuous",
      "label": "Remuneração média",
      "expression": "media_remuneracao"
    },
    "media_remuneracao_ajustada": {
      "type": "continuous",
      "label": "Remuneração média ajustada",
      "expression": "media_remuneracao_ajustada"
    }
  }
}