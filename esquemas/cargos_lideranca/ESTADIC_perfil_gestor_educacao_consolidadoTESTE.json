{
  "view": {
    "type": "data_series",
    "range": [
      {
        "label": "Total de vínculos",
        "value": "quantidade_sum"
      }
    ],
    "domain": "ano",
    "groupBy": [
      "genero",
      "cor_raca",
      "faixa_etaria"
    ],
    "filterBy": [
      "genero",
      "cor_raca",
      "faixa_etaria"
    ],
    "chartType": "bars",
    "dataSourceType": "aggregation"
  },
  "variables": {
    "ano": {
      "type": "categorical",
      "label": "Ano",
      "options": [
        "2018",
        "2021"
      ],
      "phrasePrefix": "em"
    },
    "genero": {
      "type": "categorical",
      "label": "Gênero",
      "options": [
        "Feminino",
        "Masculino"
      ],
      "phrasePrefix": "do sexo"
    },
    "cor_raca": {
      "type": "categorical",
      "label": "Cor/raça",
      "options": [
        "Branca",
          "Preta",
          "Parda",
          "Indígena"
         
      ],
      "phrasePrefix": "que atuam na esfera"
    },
    "quantidade": {
      "type": "continuous",
      "label": "Quantidade de vínculos"
    },
    "faixa_etaria": {
      "type": "categorical",
      "label": "Faixa etária",
      "options": [
        "Entre 30-49",
        "Entre 50-64",
        "Acima de 65"
      ],
      "phrasePrefix": "com idade"
    }
  }
}