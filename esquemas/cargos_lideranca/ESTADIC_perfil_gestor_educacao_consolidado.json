

{
  "view": {
    "type": "data_series",
    "range": [
      {
        "label": "Total de vínculos",
        "value": "quantidade_sum"


      }
    ],
    "domain": "ano",
    "groupBy": [
      "genero",
      "cor_raca",
      "faixa_etaria"
    ],
    "filterBy": [
      
      "genero",
      "cor_raca",
      "faixa_etaria"
    ],
    "chartType": "bars",
    
    "dataSourceType": "aggregation"
  },
  "variables": {
    "ano": {
      "type": "categorical",
      "label": "Ano",
      "options": [
        "2018",
        "2021"
      ],
      "phrasePrefix": "em"
    },
    "genero": {
      "type": "categorical",
      "label": "Gênero",
      "options": [
        "Feminino",
        "Masculino"
      ],
      "phrasePrefix": "do sexo"
    },
    "cor_raca": {
      "type": "categorical",
      "label": "Cor/raça",
      "options": [
        {
          "id": "Branca",
          "color": "dark-blue",
          "label": "Branca"
        },
        {
          "id": "Preta",
          "color": "orange",
          "label": "Preta"
        },
        {
          "id": "Parda",
          "color": "green",
          "label": "Parda"
        },
        {
          "id": "Indígena",
          "color": "light-grey",
          "label": "Indígena"
        }
      ],
      "phrasePrefix": "que atuam na esfera"
    },
    
    
    "faixa_etaria": {
      "type": "categorical",
      "label": "Faixa etária",
      "options": [
        "Entre 18 e 29 anos",
        "Entre 30 e 49 anos",
        "Entre 50 e 65 anos",
        "Acima de 65 anos",
        "Ignorado"
      ],
      "phrasePrefix": "com idade"
    },
    
    "quantidade": {
      "type": "continuous",
      "label": "Quantidade de vínculos"
    }
  }
}